{
  "version": 2,
  "builds": [{ "src": "server.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/insert/.*",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/delete/.*",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/drop/.*",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/create/.*",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/find/.*",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/update/.*",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/get/.*",
      "dest": "/server.js",
      "methods": ["POST"]
    }
  ]
}
